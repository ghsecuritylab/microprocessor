
.data
	result: .byte 0
	XYxor: .word
.text
.global main
	.equ X, 0x55AA
	.equ Y, 0xAA55
hamm:
	// XYxor = X ^ Y and it should be divided to 0 at last to calculate hamming distance
	eor r1, r1, r0
	ldr r2, =XYxor
	str r1, [r2]
	// do "and gate" to determine whether to add 1
	ldr r1, =xor_res
	movs r1, #1
	and r1, r1, #1
	// add and store the value to result
	ldr r2, =result
	add r2, r2, r1
	ldr r3, =result
	str r2, [r3]

	bx lr
main:
	ldr r0, =#X //This code will cause assemble error. Why? And how to fix.
	ldr r1, =#Y
	ldr r2, =result
	bl hamm
L: b L
