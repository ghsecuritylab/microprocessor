	.syntax unified
	.cpu cortex-m4
	.thumb
.data
	leds: .byte 0
.text
	.global main
	.equ RCC_AHB2ENR, 0x4002104C
	.equ GPIOA_MODER, 0x48000400
	.equ GPIOA_OTYPER, 0x48000404
	.equ GPIOA_OSPEEDR, 0x48000408
	.equ GPIOA_PUPDR, 0x4800040C
	.equ GPIOA_ODR, 0x48000414
main:
	movs r0, #0x1
	ldr r1, =RCC_AHB2ENR
	str r0, [r1]

	movs r0, #0x400
	ldr r1, =GPIOA_MODER
	ldr r2, [r1]
	and r2, #0xFFFFF3FF
	orrs r2, r2, r0
	str r2, [r1]

	movs r0, #0x800
	ldr r1, =GPIOA_OSPEEDR
	strh r0, [r1]

	ldr r1, =GPIOA_ODR

L1:
	movs r0, #(1<<5)
	strh r0, [r1]

	B L1
/*
	BL GPIO_init
	MOVS R1, #1
	LDR R0, =leds
	STRB R1, [R0]
Loop:
	//TODO: Write the display pattern into leds variable
	BL DisplayLED
	BL Delay
	B Loop
