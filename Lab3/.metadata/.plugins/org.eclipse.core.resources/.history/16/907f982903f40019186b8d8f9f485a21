	.syntax unified
	.cpu cortex-m4
	.thumb
.data
	leds: .byte 0
.text
	.global main
main:
	BL GPIO_init
	MOVS R1, #1
	LDR R0, =leds
	STRB R1, [R0]
Loop:
	//TODO: Write the display pattern into leds variable
	BL DisplayLED
	BL Delay
	B Loop
GPIO_init:
	//TODO: Initial LED GPIO pins as output
	BX LR
	DisplayLED :
	//TODO: Display LED by leds
	BX LR
Delay:
	//TODO: Write a delay 1 sec function
	BX LR
